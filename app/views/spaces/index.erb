<h3>Welcome <%=current_user.name%>
<ul id='listed_spaces'>
  <% @spaces.each do |space| %>
   <li> Name: <%= space.space_name %>
    </br>
    Description: <%= space.description %></br>
    Price per night: <%= space.price %></li>
    </br>
    Available dates: <%=space.start_date %> - <%=space.end_date%>
    </br>
  </br>
 <% end %>
</ul>
<form action='/session' method='post'>
  <input type='hidden' name='_method' value='delete'>
  <input type='submit' value='Sign Out'>

</form>
